#%RAML 1.0 DataType
type : object

properties:
  first_name: 
    type: string
    minLength: 4
    maxLength: 20
  last_name:
    type: string
    minLength: 4
    maxLength: 20
  email: 
    type: string
    pattern: ^.+@.+\..+$
  phone: 
    type: string
    minLength: 10
    maxLength: 10
  date_of_birth: date-only
  gender: integer
  employee_type: string
  annual_income: string
  active:
    type: string
    enum: [active,inactive]
      